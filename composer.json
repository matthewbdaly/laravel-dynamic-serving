{
    "name": "matthewbdaly/laravel-dynamic-serving",
    "description": "Dynamic serving middleware for Laravel",
    "type": "library",
    "keywords": ["laravel","lumen"],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "3.5",
        "orchestra/testbench-browser-kit": "~3.5",
        "phpunit/phpunit": "^6.3",
        "mockery/mockery": "~1.0",
        "squizlabs/php_codesniffer": "^3.1",
        "satooshi/php-coveralls": "^1.0",
        "psy/psysh": "0.8.*",
        "matthewbdaly/artisan-standalone": "0.0.*",
        "sebastian/phpcpd": "^3.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelDynamicServing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
